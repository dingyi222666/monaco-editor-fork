{
	"defaultToken": "",
	"tokenPostfix": ".pla",
	"brackets": [
		{
			"open": "[",
			"close": "]",
			"token": "delimiter.square"
		},
		{
			"open": "<",
			"close": ">",
			"token": "delimiter.angle"
		},
		{
			"open": "(",
			"close": ")",
			"token": "delimiter.parenthesis"
		}
	],
	"keywords": [
		".i",
		".o",
		".mv",
		".ilb",
		".ob",
		".label",
		".type",
		".phase",
		".pair",
		".symbolic",
		".symbolic-output",
		".kiss",
		".p",
		".e",
		".end"
	],
	"comment": {},
	"identifier": {},
	"plaContent": {},
	"tokenizer": {
		"root": [
			{
				"include": "@whitespace"
			},
			[{}, "comment"],
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "keyword.$1"
						},
						"@keywords": {
							"cases": {
								".type": {
									"token": "keyword.$1",
									"next": "@type"
								},
								"@default": {
									"token": "keyword.$1",
									"next": "@keywordArg"
								}
							}
						},
						"@default": {
							"token": "keyword.$1"
						}
					}
				}
			],
			[{}, "identifier"],
			[{}, "string"]
		],
		"whitespace": [[{}, ""]],
		"type": [
			{
				"include": "@whitespace"
			},
			[
				{},
				{
					"token": "type",
					"next": "@pop"
				}
			]
		],
		"keywordArg": [
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "",
							"next": "@pop"
						},
						"@default": ""
					}
				}
			],
			[{}, "comment", "@pop"],
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "@brackets",
							"next": "@pop"
						},
						"@default": "@brackets"
					}
				}
			],
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "number",
							"next": "@pop"
						},
						"@default": "number"
					}
				}
			],
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "identifier",
							"next": "@pop"
						},
						"@default": "identifier"
					}
				}
			],
			[
				{},
				{
					"cases": {
						"@eos": {
							"token": "delimiter",
							"next": "@pop"
						},
						"@default": "delimiter"
					}
				}
			]
		]
	}
}
