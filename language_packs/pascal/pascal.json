{
	"defaultToken": "",
	"tokenPostfix": ".pascal",
	"ignoreCase": true,
	"brackets": [
		{
			"open": "{",
			"close": "}",
			"token": "delimiter.curly"
		},
		{
			"open": "[",
			"close": "]",
			"token": "delimiter.square"
		},
		{
			"open": "(",
			"close": ")",
			"token": "delimiter.parenthesis"
		},
		{
			"open": "<",
			"close": ">",
			"token": "delimiter.angle"
		}
	],
	"keywords": [
		"absolute",
		"abstract",
		"all",
		"and_then",
		"array",
		"as",
		"asm",
		"attribute",
		"begin",
		"bindable",
